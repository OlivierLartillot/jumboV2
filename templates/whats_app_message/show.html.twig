<!DOCTYPE html>

<title>WhatsAppMessage</title>

{% block body %}
    <h1>WhatsAppMessage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ whats_app_message.id }}</td>
            </tr>
            <tr>
                <th>TypeTransfer</th>
                <td>{{ whats_app_message.typeTransfer }}</td>
            </tr>
            <tr>
                <th>Language</th>
                <td>{{ whats_app_message.language }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ whats_app_message.message }}</td>
            </tr>
            <tr>
                <th>IsDefaultMessage</th>
                <td>{{ whats_app_message.isDefaultMessage ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_whats_app_message_index') }}">back to list</a>

    <a href="{{ path('app_whats_app_message_edit', {'id': whats_app_message.id}) }}">edit</a>

    {{ include('whats_app_message/_delete_form.html.twig') }}
{% endblock %}
