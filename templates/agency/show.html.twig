<!DOCTYPE html>

<title>Agency</title>

{% block body %}
    <h1>Agency</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agency.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ agency.name }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ agency.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Language</th>
                <td>{{ agency.language }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_agency_index') }}">back to list</a>

    <a href="{{ path('app_agency_edit', {'id': agency.id}) }}">edit</a>

    {{ include('agency/_delete_form.html.twig') }}
{% endblock %}
